@model List<AntrenorAddModel>

@{
    ViewData["Title"] = "Admin";
    Layout = "AdminLayout";
}

<head>
    <link rel="stylesheet" href="~/css/AdminPanelIndex.css" />
</head>


<table class="table">
    <thead>
        <tr>
            <th>
                İsim
            </th>
            <th>
                Soyisim
            </th>
            <th>
                Eposta
            </th>
            <th>
                Uzmanlık Alanı
            </th>
            <th>
                Deneyimleri
            </th>
        </tr>
    </thead>
    <tbody>
        @for (int i = 0; i < Model.Count; i++)
        {
            <tr>
                <td>
                    @Model[i].FirstName
                </td>
                <td>
                    @Model[i].LastName
                </td>
                <td>
                    @Model[i].Email
                </td>
                <td>
                    @Model[i].UzmanlikAlanlari
                </td>
                <td>
                    @Model[i].Deneyimleri
                </td>
            </tr>
        }
    </tbody>
</table>